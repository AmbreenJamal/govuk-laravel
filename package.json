{
    "name": "govuk-laravel",
    "description": "Use the GOV.UK Design System within the Laravel ecosystem, complete with Blade components and templates!",
    "keywords": [
        "laravel",
        "gov.uk",
        "design system",
        "blade component",
        "blade template"
    ],
    "author": "Anthony Edmonds",
    "license": "MIT",
    "homepage": "https://github.com/AnthonyEdmonds/govuk-laravel",
    "scripts": {
        "test": "php vendor/bin/phpunit",
        "php-stan": "php vendor/bin/phpstan analyse -c phpstan.neon --memory-limit 512M",
        "pint": "php ./vendor/bin/pint",
        "prepare": "husky install"
    },
    "devDependencies": {
        "govuk-frontend": "^4.6",
        "husky": "^8"
    }
}
